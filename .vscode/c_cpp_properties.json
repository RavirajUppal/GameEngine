{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/Engine/include",
                "${workspaceFolder}/Engine/src",
                "${workspaceFolder}/Editor/src",
                "${workspaceFolder}/Game/src",
                "${workspaceFolder}/**"
            ],
            "defines": [
                "REALENGINE_BUILD_DLL",
                "IMGUI_IMPL_OPENGL_LOADER_CUSTOM",
                "IS_ENGINE_BUILD",
                "IS_GAME_BUILD",
                "IS_EDITOR_BUILD",
                "SHADER_DIR=\"src/Resources/Shaders/\"",
                "TEXTURE_DIR=\"src/Resources/Textures/\""

            ],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-x64"
        }
    ],
    "version": 4
}